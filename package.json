  "description": "AI workflow orchestration platform with visual builder, security scanning, and multi-model routing",
  "author": "Workflow Dashboard Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/holburn-hornssentinel/workflow-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/holburn-hornssentinel/workflow-dashboard/issues"
  },
  "homepage": "https://github.com/holburn-hornssentinel/workflow-dashboard#readme",
  "keywords": [
    "ai",
    "workflow",
    "orchestration",
    "automation",
    "claude",
    "gemini",
    "llm",
    "security",
    "mcp",
    "nextjs"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3004",
    "dev:restart": "./dev.sh",
    "build": "next build",
    "start": "next start -p 3004",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest __tests__/lib",
    "test:api": "jest __tests__/api",
    "test:component": "jest __tests__/components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run type-check && npm run lint && npm run test:ci && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
